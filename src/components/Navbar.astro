---
import Logo from '../assets/qr.png'

const navItems = [
  {title: "Projects", url: "#Projects"},
  {title: "Profile", url: "#Profile"}
]
---

<nav class=" flex justify-between items-center max-w-xs mx-auto	 px-4 backdrop-blur-md bg-black bg-opacity-10 hover:border-emerald-500 sticky top-5 z-50  rounded-xl border-2 border-borderWindow">
  <a href="#home" class="cursor-pointer" onclick="goToTop()">
    <img class="w-20" src={Logo} alt="">
  </a>
  <section>
    <ul class=" flex justify-center gap-4 text-white cursor-pointer font-bold">
      {navItems.map(({title, url}) => (
        <li>
          <a class=" bg-clip-text text-transparent bg-gradient-to-r from-pink-500 via-red-500 to-yellow-500 animate-text
      
      
      " href={url}>{title}</a></li>
      ))}
    </ul>
  </section>
</nav>




